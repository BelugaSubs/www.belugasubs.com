{
  "name": "belugasubs_official_website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "build-ci": "hexo clean && hexo generate",
    "dev": "hexo clean && hexo generate && hexo server --open",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "postinstall": "husky || true"
  },
  "hexo": {
    "version": "8.1.0"
  },
  "dependencies": {
    "hexo": "^8.1.0",
    "hexo-filter-nofollow": "^2.0.2",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^7.0.1",
    "hexo-renderer-stylus": "^3.0.0",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.1.0"
  },
  "devDependencies": {
    "@zl-asica/prettier-config": "^1.0.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue,css,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.19.0"
}
